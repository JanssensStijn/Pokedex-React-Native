{
  "expo": {
    "name": "Pokedex",
    "slug": "Pokedex",
    "description": "A Pokedex made with React Native",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pokemon_not_found.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pokemon_not_found.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pokemon_not_found.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "be.snipersoft.pokedex"
    },
    "web": {
      "favicon": "./assets/pokemon_not_found.png"
    },
    "plugins": [
      "expo-audio"
    ],
    "extra": {
      "eas": {
        "projectId": "13df90a6-c189-4260-aafd-b35367531fde"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/13df90a6-c189-4260-aafd-b35367531fde"
    }
  }
}